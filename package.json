{
  "name": "zyndex-server",
  "version": "0.2.0-30",
  "description": "Backend Server for Handling Zyndex Frontend",
  "main": "app.js",
  "scripts": {
    "prepare": "husky install && husky add .husky/commit-msg \"npx --no-install commitlint --edit \"$1\"\"",
    "git:stage": "git add",
    "git:commit": "cz",
    "git:push": "git push --follow-tags origin dev",
    "release:pre": "standard-version --prerelease --sign",
    "release:patch": "standard-version --release-as patch --sign",
    "release:minor": "standard-version --release-as minor --sign",
    "release:major": "standard-version --release-as major --sign",
    "test": "set NODE_ENV=test && ts-mocha --paths tests/**/*.spec.ts -r 'tests/require'",
    "test:api": "set NODE_ENV=test && ts-mocha --paths tests/api/**/*.spec.ts -r 'tests/require'",
    "test:unit": "set NODE_ENV=test && ts-mocha --paths tests/unit/**/*.spec.ts -r 'tests/require'",
    "test:single": "set NODE_ENV=test && ts-mocha -r 'tests/require'",
    "test:report": "set NODE_ENV=test && ts-mocha --paths tests/api/**/*.spec.ts -r 'tests/require' --reporter mocha-simple-html-reporter --reporter-options output=test-report.html",
    "serve:dev": "set NODE_ENV=development && nodemon --ignore tests/ -r tsconfig-paths/register app.ts",
    "build:tsc": "set NODE_ENV=production && tsc && tsc-alias",
    "build:prepare": "bash ./scripts/prepare-build.sh",
    "start": "set NODE_ENV=production && node app.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zyndex-drive/server.git"
  },
  "keywords": [
    "Nodejs",
    "Backend",
    "Google",
    "Drive",
    "mongodb",
    "oauth2"
  ],
  "author": "tks18",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zyndex-drive/server/issues"
  },
  "homepage": "https://github.com/zyndex-drive/server#readme",
  "dependencies": {
    "@godaddy/terminus": "^4.9.0",
    "axios": "^0.24.0",
    "bcrypt": "^5.0.1",
    "crypto-js": "^4.1.1",
    "dot-prop": "^6.0.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-mongo-sanitize": "^2.1.0",
    "got": "^11.8.3",
    "helmet": "^4.6.0",
    "jose": "^4.3.7",
    "jsonwebtoken": "^8.5.1",
    "md5": "^2.3.0",
    "mongoose": "^5.13.13",
    "nanoid": "^3.1.30",
    "node-webcrypto-ossl": "^2.1.3",
    "nodemailer": "^6.7.1",
    "query-string": "^7.0.1",
    "sha1": "^1.1.1",
    "uuid": "^8.3.2",
    "x-xss-protection": "^2.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.2.1",
    "@commitlint/config-conventional": "^13.2.0",
    "@types/bcrypt": "^5.0.0",
    "@types/chai": "^4.2.22",
    "@types/chai-as-promised": "^7.1.4",
    "@types/crypto-js": "^4.0.2",
    "@types/dotenv": "^8.2.0",
    "@types/expect": "^24.3.0",
    "@types/express": "^4.17.13",
    "@types/http-server": "^0.12.1",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/md5": "^2.3.1",
    "@types/mocha": "^8.2.3",
    "@types/node": "^15.14.9",
    "@types/nodemailer": "^6.4.4",
    "@types/sha1": "^1.1.3",
    "@types/uuid": "^8.3.3",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "chai-each": "^0.0.1",
    "chai-http": "^4.3.0",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.32.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-nodejs": "^2.0.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsdoc": "^35.5.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.1",
    "husky": "^7.0.4",
    "jsdoc": "^3.6.7",
    "mocha": "^9.1.3",
    "mocha-simple-html-reporter": "^2.0.0",
    "mocha-steps": "^1.3.0",
    "standard-version": "^9.3.2",
    "ts-mocha": "^8.0.0",
    "tsc-alias": "^1.4.1",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "packageManager": "yarn@3.1.0"
}
